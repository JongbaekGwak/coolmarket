<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="coolmarket.mapper.MessageMapper">

	<insert id="insertMessage" parameterType="com.dto.MessageDto">

		<![CDATA[ 
			INSERT INTO
				msg (buyre_no, buyer_name, buyer_no)
			VALUES
				(#{buyerNo}, #{buyerName}, #{buyerNo})
		]]>
	</insert>
	
</mapper>