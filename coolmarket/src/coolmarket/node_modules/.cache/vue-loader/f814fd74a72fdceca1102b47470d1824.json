\n                >\n                  <b-form-input\n                    id=\"userNickName\"\n                    v-model=\"userNickName\"\n                    trim\n                    placeholder=\"별명을 입력해 주세요.\"\n                    type=\"text\"\n                  ></b-form-input>\n                </b-form-group>\n              </b-col>\n            </b-row>\n\n            <!-- 역락처 -->\n            <b-form-group label=\"연락처 : \" label-for=\"userTel\" class=\"mt-3\">\n              <b-input-group>\n                <b-form-input\n                  id=\"userTel\"\n                  v-model=\"userTel\"\n                  trim\n                  placeholder=\"연락처를 입력해 주세요. ( - 는 빼고 입력해주세요.)\"\n                  type=\"text\"\n                ></b-form-input>\n              </b-input-group>\n            </b-form-group>\n\n            <!-- 주소 -->\n            <div class=\"mt-3 py-4\">\n              <p>주소 :</p>\n              <b-row>\n                <!-- 주소1 -->\n                <b-col>\n                  <b-dropdown id=\"address1\" :text=\"address1\">\n                    <b-dropdown-item\n                      v-for=\"item in addr1\"\n                      :key=\"item\"\n                      v-on:click=\"add1(item)\"\n                      ><small>{{ item }}</small>\n                    </b-dropdown-item>\n                  </b-dropdown>\n                </b-col>\n\n                <!-- 주소2 -->\n                <b-col>\n                  <b-dropdown id=\"address2\" :text=\"address2\">\n                    <b-dropdown-item\n                      v-for=\"item in addr2\"\n                      :key=\"item\"\n                      v-on:click=\"add2(item)\"\n                      ><small>{{ item }}</small>\n                    </b-dropdown-item>\n                  </b-dropdown>\n                </b-col>\n\n                <!-- 주소3 -->\n                <b-col>\n                  <b-dropdown id=\"address3\" :text=\"address3\">\n                    <b-dropdown-item\n                      v-for=\"item in addr3\"\n                      :key=\"item\"\n                      v-on:click=\"add3(item)\"\n                      ><small>{{ item }}</small>\n                    </b-dropdown-item>\n                  </b-dropdown>\n                </b-col>\n              </b-row>\n            </div>\n\n            <!-- 버튼 -->\n            <b-button-group vertical class=\"d-flex mt-2\">\n              <b-button\n                size=\"lg\"\n                pill\n                block\n                variant=\"primary\"\n                class=\"mt-2\"\n                v-on:click=\"signUp\"\n                >가입하기</b-button\n              >\n            </b-button-group>\n          </b-col>\n        </b-row>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      userId: \"\",\n      userPw: \"\",\n      userPw2: \"\",\n      userName: \"\",\n      userNickName: \"\",\n      userTel: \"\",\n      address1: \"시/도\",\n      address2: \"시/군/구\",\n      address3: \"동\",\n      addr1: {},\n      addr2: [],\n      addr3: [],\n      check: false,\n    };\n  },\n  beforeCreate() {\n    if (this.$session.get(\"coolId\") != null) {\n      this.$router.push(\"/MyPage\");\n    } else if (this.$session.get(\"manager\" == 1)) {\n      this.$router.push(\"ManagerPage\");\n    }\n  },\n  mounted() {\n    this.$axios\n      .get(\"http://localhost:9000/addr1\")\n      .then((res) => {\n        this.addr1 = res.data;\n      })\n      .catch((err) => {\n        alert(\"서버 통신 err\");\n        console.log(err);\n      });\n  },\n  methods: {\n    overlapCheck() {\n      this.$axios\n        .get(\"http://localhost:9000/overlabCheck\", {\n          params: {\n            email: this.userId,\n          },\n        })\n        .then((res) => {\n          if (res.data == true) {\n            alert(\"사용할 수 있는 아이디 입니다.\");\n            this.check = true;\n          } else {\n            alert(\"사용할 수 없는 아이디 입니다.\");\n          }\n        })\n        .catch((err) => {\n          alert(\"서버 통신 err\");\n          console.log(err);\n        });\n    },\n    signUp() {\n      if (this.check == false) {\n        alert(\"이메일 중복확인 해주세요\");\n      } else {\n        if (this.userPw == \"\") {\n          alert(\"비밀번호를 입력해 주세요\");\n        } else if (this.userPw2 == \"\") {\n          alert(\"비밀번호 확인을 입력해 주세요\");\n        } else if (this.userName == \"\") {\n          alert(\"이름을 입력해 주세요\");\n        } else if (this.userNickName == \"\") {\n          alert(\"별명을 입력해 주세요\");\n        } else if (this.userTel == \"\") {\n          alert(\"연락처를 입력해 주세요\");\n        } else if (\n          this.address1 == \"\" ||\n          this.address2 == \"\" ||\n          this.address3 == \"\"\n        ) {\n          alert(\"주소를 선택해 주세요\");\n        } else {\n          if (this.userPw != this.userPw2) {\n            alert(\"비밀번호를 확인해 주세요\");\n          } else {\n            this.$axios\n              .post(\"http://localhost:9000/signUp\", {\n                email: this.userId,\n                password: this.userPw,\n                name: this.userName,\n                nickName: this.userNickName,\n                tel: this.userTel,\n                addr1: this.address1,\n                addr2: this.address2,\n                addr3: this.address3,\n              })\n              .then(() => {\n                alert(\"회원가입 완료\");\n                this.$router.push(\"/login\");\n              })\n              .catch((err) => {\n                alert(\"서버 통신 err\");\n                console.log(err);\n              });\n          }\n        }\n      }\n    },\n    add1(str) {\n      this.address1 = str;\n      this.$axios\n        .get(\"http://localhost:9000/addr2\", {\n          params: {\n            addr1: this.address1,\n          },\n        })\n        .then((res) => {\n          this.addr2 = res.data;\n        })\n        .catch((err) => {\n          alert(\"서버 통신 err\");\n          console.log(err);\n        });\n    },\n    add2(str) {\n      this.address2 = str;\n      this.$axios\n        .get(\"http://localhost:9000/addr3\", {\n          params: {\n            addr1: this.address1,\n            addr2: this.address2,\n          },\n        })\n        .then((res) => {\n          this.addr3 = res.data;\n        })\n        .catch((err) => {\n          alert(\"서버 통신 err\");\n          console.log(err);\n        });\n    },\n    add3(str) {\n      this.address3 = str;\n    },\n  },\n};\n</script>\n<style>\n.d {\n  max-height: 1000px;\n  overflow: auto;\n}\n</style>"]}]}