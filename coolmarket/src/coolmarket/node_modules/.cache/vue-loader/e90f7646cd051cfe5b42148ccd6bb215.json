d=\"userId\"\n                v-model=\"userId\"\n                trim\n                placeholder=\"이메일을 입력해 주세요.\"\n                type=\"text\"\n              ></b-form-input>\n            </b-form-group>\n            <b-form-group label=\"패스워드 : \" label-for=\"userPw\" class=\"mt-3\">\n              <b-form-input\n                id=\"userPw\"\n                v-model=\"userPw\"\n                trim\n                placeholder=\"패스워드를 압력해 주세요.\"\n                v-on:keyup.enter=\"login\"\n                type=\"password\"\n              ></b-form-input>\n            </b-form-group>\n            <p class=\"mt-3 text-info cursor\" v-on:click=\"pwFind\">\n              패스워드를 잊어 버리셧나요?\n            </p>\n            <b-button-group vertical class=\"d-flex\">\n              <b-button\n                size=\"lg\"\n                pill\n                block\n                variant=\"primary\"\n                class=\"mt-3\"\n                v-on:click=\"login\"\n                >로그인</b-button\n              >\n              <b-button\n                size=\"lg\"\n                pill\n                block\n                variant=\"success\"\n                class=\"mt-2\"\n                v-on:click=\"signUp\"\n                >회원가입</b-button\n              >\n            </b-button-group>\n\n            <!-- 비밀번호 찾기 -->\n            <div class=\"mt-5\" v-if=\"findShow == true\">\n              <hr />\n              <h3 class=\"text-center\">비밀번호 찾기</h3>\n              <b-form-group label=\"이메일 : \" label-for=\"userId\">\n                <b-form-input\n                  id=\"findId\"\n                  v-model=\"findId\"\n                  trim\n                  placeholder=\"가입하신 이메일을 입력해 주세요.\"\n                  type=\"text\"\n                ></b-form-input>\n              </b-form-group>\n              <b-form-group label=\"이름 : \" label-for=\"findName\">\n                <b-form-input\n                  id=\"findName\"\n                  v-model=\"findName\"\n                  trim\n                  placeholder=\"가입하신 이름을 입력해 주세요.\"\n                  type=\"text\"\n                ></b-form-input>\n              </b-form-group>\n              <b-form-group label=\"연락처 : \" label-for=\"findTel\">\n                <b-form-input\n                  id=\"findTel\"\n                  v-model=\"findTel\"\n                  trim\n                  placeholder=\"가입하신 연락처를 입력해 주세요.\"\n                  type=\"text\"\n                ></b-form-input>\n              </b-form-group>\n              <b-button-group vertical class=\"d-flex\">\n                <b-button\n                  size=\"lg\"\n                  pill\n                  block\n                  variant=\"info\"\n                  class=\"mt-3\"\n                  v-on:click=\"find\"\n                  >비밀번호 찾기</b-button\n                >\n              </b-button-group>\n            </div>\n          </b-col>\n        </b-row>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      userId: \"\",\n      userPw: \"\",\n      findShow: false,\n      findId: \"\",\n      findName: \"\",\n      findTel: \"\",\n    };\n  },\n  beforeCreate() {\n    if (this.$session.get(\"coolUserNo\") == null) {\n      this.$router.push(\"/Login\");\n    } else {\n      this.$router.push(\"MyPage\");\n    }\n  },\n  methods: {\n    login() {\n      if (this.userId == \"\") {\n        alert(\"아이디를 입력하세요\");\n      } else if (this.userPw == \"\") {\n        alert(\"비밀번호를 입력하세요\");\n      } else {\n        this.$axios\n          .post(\"http://localhost:9000/login\", null, {\n            params: {\n              email: this.userId,\n              password: this.userPw,\n            },\n          })\n          .then((res) => {\n            if (res.data == false) {\n              alert(\"아이디, 비밀번호를 확인해 주세요\");\n            } else {\n              this.$session.set(\"coolUserNo\", res.data.userNo);\n              this.$session.set(\"coolEmail\", res.data.email);\n              this.$session.set(\"coolRank\", res.data.rank);\n              this.$session.set(\"coolNickName\", res.data.nickName);\n              if (\n                this.$route.query.toSignUp != 1 ||\n                this.$route.query.toSignUp == undefined\n              ) {\n                this.$router.go(-1);\n              } else {\n                this.$router.push(\"/\");\n              }\n            }\n          })\n          .catch((err) => {\n            console.log(err);\n          });\n      }\n    },\n    signUp() {\n      this.$router.push(\"SignUp\");\n    },\n    pwFind() {\n      this.findShow = true;\n    },\n    find() {\n      this.$axios\n        .post(\"http://localhost:9000/find\", {\n          email: this.findId,\n          name: this.findName,\n          tel: this.findTel,\n        })\n        .then((res) => {\n          if (res.data == \"\") {\n            alert(\"입력하신 정보의 사용자가 없습니다. \\n 다시 입력해 주세요.\");\n          } else {\n            alert(\"비밀번호 : \" + res.data + \" 입니다.\");\n            this.findId = \"\";\n            this.findName = \"\";\n            this.findTel = \"\";\n            this.findShow = false;\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n  },\n};\n</script>\n<style scoped>\n.cursor {\n  cursor: pointer;\n}\n</style>"]}]}