\"mt-3 pb-3 border-bottom\">마켓 물건 등록하기</h2>\r\n\r\n    <div class=\"d-lg-flex justify-content-between d-block\">\r\n      <div class=\"mt-3\">\r\n        <span class=\"me-1\">카테고리 :</span>\r\n        <b-form-select\r\n          v-model=\"marCate\"\r\n          :options=\"cate\"\r\n          class=\"btn btn-sm btn-warning\"\r\n        ></b-form-select>\r\n      </div>\r\n      <div class=\"mt-3\">\r\n        <span class=\"me-1\">주소 : </span>\r\n        <b-form-select\r\n          v-on:change=\"addre2\"\r\n          v-model=\"address1\"\r\n          :options=\"addr1\"\r\n          class=\"btn btn-sm btn-primary\"\r\n        ></b-form-select>\r\n        <b-form-select\r\n          v-on:change=\"addre3\"\r\n          v-model=\"address2\"\r\n          :options=\"addr2\"\r\n          class=\"btn btn-sm btn-info\"\r\n        ></b-form-select>\r\n        <b-form-select\r\n          v-model=\"address3\"\r\n          :options=\"addr3\"\r\n          class=\"btn btn-sm btn-success\"\r\n        ></b-form-select>\r\n      </div>\r\n    </div>\r\n\r\n    <b-row class=\"mt-3\">\r\n      <b-col>\r\n        <div class=\"input-group\">\r\n          <div class=\"input-group-text\">제목</div>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"title\"\r\n            name=\"title\"\r\n            placeholder=\"제목을 입력하세요\"\r\n            v-model=\"title\"\r\n          />\r\n        </div>\r\n      </b-col>\r\n\r\n      <b-col>\r\n        <div class=\"input-group\">\r\n          <div class=\"input-group-text\">가격</div>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"price\"\r\n            name=\"price\"\r\n            placeholder=\"가격을 입력하세요\"\r\n            v-model=\"price\"\r\n          />\r\n        </div>\r\n      </b-col>\r\n    </b-row>\r\n\r\n    <div class=\"mt-3\">\r\n      <input\r\n        type=\"file\"\r\n        class=\"form-control\"\r\n        multiple\r\n        accept=\"image/*\"\r\n        @change=\"up\"\r\n      />\r\n    </div>\r\n\r\n    <div class=\"form-group mt-3\">\r\n      <textarea\r\n        name=\"contents\"\r\n        id=\"contents\"\r\n        rows=\"10\"\r\n        class=\"form-control\"\r\n        v-model=\"contents\"\r\n      ></textarea>\r\n    </div>\r\n    <div class=\"d-flex justify-content-end mt-3\">\r\n      <button\r\n        type=\"button\"\r\n        class=\"btn btn-primary mx-1\"\r\n        v-on:click=\"moveMarketList\"\r\n      >\r\n        목록으로\r\n      </button>\r\n      <button\r\n        type=\"button\"\r\n        class=\"btn btn-success mx-1\"\r\n        v-on:click=\"marketWrite\"\r\n      >\r\n        글쓰기\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      title: \"\",\r\n      price: \"\",\r\n      images: \"\",\r\n      contents: \"\",\r\n      address1: null,\r\n      address2: null,\r\n      address3: null,\r\n      marCate: null,\r\n      addr1: [],\r\n      addr2: [{ value: null, text: \"시/군/구\" }],\r\n      addr3: [{ value: null, text: \"동\" }],\r\n      cate: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$axios\r\n      .get(\"http://localhost:9000/addr1\")\r\n      .then((res) => {\r\n        this.addr1 = res.data;\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n    this.$axios\r\n      .get(\"http://localhost:9000/marCate\")\r\n      .then((res) => {\r\n        this.cate = res.data;\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  },\r\n  methods: {\r\n    addre2() {\r\n      this.address2 = null;\r\n      this.address3 = null;\r\n      this.$axios\r\n        .get(\"http://localhost:9000/addr2\", {\r\n          params: {\r\n            addr1: this.address1,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          if (res.data.length == 1) {\r\n            this.addr2 = [\r\n              { value: null, text: \"시/군/구\" },\r\n              { value: \"없음\", text: \"없음\" },\r\n            ];\r\n          } else {\r\n            this.addr2 = res.data;\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    addre3() {\r\n      this.address3 = null;\r\n      this.$axios\r\n        .get(\"http://localhost:9000/addr3\", {\r\n          params: {\r\n            addr1: this.address1,\r\n            addr2: this.address2,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          if (res.data.length == 1) {\r\n            this.addr3 = [\r\n              { value: null, text: \"동\" },\r\n              { value: \"없음\", text: \"없음\" },\r\n            ];\r\n          } else {\r\n            this.addr3 = res.data;\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    moveMarketList() {\r\n      this.$router.push(\"/MarketList\");\r\n    },\r\n    marketWrite() {\r\n      if (this.title == \"\") {\r\n        alert(\"제목을 입력해 주세요\");\r\n      } else if (this.price == \"\") {\r\n        alert(\"가격을 입력해 주세요\");\r\n      } else if (this.marCate == null) {\r\n        alert(\"카테고리를 선택해 주세요\");\r\n      } else if (this.address1 == null) {\r\n        alert(\"주소를 선택해 주세요\");\r\n      } else {\r\n        let marprice = parseInt(this.price);\r\n        if (isNaN(marprice)) {\r\n          alert(\"가격은 숫자만 입력할 수 있습니다.\");\r\n        } else {\r\n          this.$axios\r\n            .get(\"http://localhost:9000/marketWrite\", {\r\n              params: {\r\n                marTitle: this.title,\r\n                marPrice: marprice,\r\n                marContents: this.contents,\r\n                marAddr1: this.address1,\r\n                marAddr2: this.address2,\r\n                marAddr3: this.Address3,\r\n                marCate: this.marCate,\r\n                marUserNo: this.$session.get(\"coolUserNo\"),\r\n                marCreaNickName: this.$session.get(\"coolNickName\"),\r\n              },\r\n            })\r\n            .then((res) => {\r\n              if (this.images != \"\") {\r\n                let fromData = new FormData();\r\n                for (let i = 0; i < this.images.length; i++) {\r\n                  fromData.append(\"image\", this.images[i]);\r\n                }\r\n                this.$axios\r\n                  .post(\"http://localhost:9000/imgInsert\", fromData, {\r\n                    params: { marNo: res.data, comNo: \"\" },\r\n                    headers: {\r\n                      \"Content-Type\": \"multipart/form-data\",\r\n                    },\r\n                  })\r\n                  .then(() => {\r\n                    alert(\"작성완료\");\r\n                    this.$router.push({\r\n                      name: \"MarketDetail\",\r\n                      query: { marNo: res.data },\r\n                    });\r\n                  })\r\n                  .catch((err) => {\r\n                    console.log(err);\r\n                  });\r\n              } else {\r\n                alert(\"작성완료\");\r\n                this.$router.push({\r\n                  name: \"MarketDetail\",\r\n                  query: { marNo: res.data },\r\n                });\r\n              }\r\n            })\r\n            .catch((err) => {\r\n              console.log(err);\r\n            });\r\n        }\r\n      }\r\n    },\r\n    up(file) {\r\n      this.images = file.target.files;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>"]}]}