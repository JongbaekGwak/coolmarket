sourceRoot":"src/views","sourcesContent":["<template>\n  <div>\n    <b-row>\n      <b-col md=\"8\" class=\"mx-auto border py-5\">\n        <h1 class=\"text-center mt-3\">회원가입하기</h1>\n        <b-row class=\"mt-2\">\n          <b-col md=\"10\" class=\"mx-auto my-5\">\n            <!-- 아이디 -->\n            <b-form-group label=\"이메일 : \" label-for=\"userId\">\n              <b-input-group>\n                <b-form-input\n                  id=\"userId\"\n                  v-model=\"userId\"\n                  trim\n                  placeholder=\"이메일을 입력해 주세요.\"\n                  type=\"email\"\n                ></b-form-input>\n                <b-input-group-text v-on:click=\"overlapCheck\" type=\"button\"\n                  >중복확인</b-input-group-text\n                >\n              </b-input-group>\n            </b-form-group>\n\n            <!-- 비밀번호 -->\n            <b-row>\n              <b-col>\n                <b-form-group\n                  label=\"패스워드 : \"\n                  label-for=\"userPw\"\n                  class=\"mt-3\"\n                >\n                  <b-form-input\n                    id=\"userPw\"\n                    v-model=\"userPw\"\n                    trim\n                    placeholder=\"패스워드를 입력해 주세요.\"\n                    type=\"password\"\n                  ></b-form-input>\n                </b-form-group>\n              </b-col>\n              <b-col>\n                <b-form-group\n                  label=\"패스워드 확인 : \"\n                  label-for=\"userPw2\"\n                  class=\"mt-3\"\n                >\n                  <b-form-input\n                    id=\"userPw2\"\n                    v-model=\"userPw2\"\n                    trim\n                    placeholder=\"패스워드를 입력해 주세요.\"\n                    type=\"password\"\n                  ></b-form-input>\n                </b-form-group>\n              </b-col>\n            </b-row>\n\n            <!-- 이름, 닉네임 -->\n            <b-row>\n              <b-col>\n                <b-form-group label=\"이름 : \" label-for=\"userName\" class=\"mt-3\">\n                  <b-form-input\n                    id=\"userName\"\n                    v-model=\"userName\"\n                    trim\n                    placeholder=\"이름을 입력해 주세요.\"\n                    type=\"text\"\n                  ></b-form-input>\n                </b-form-group>\n              </b-col>\n              <b-col>\n                <b-form-group\n                  label=\"별명 : \"\n                  label-for=\"userNickName\"\n                  class=\"mt-3\"\n                >\n                  <b-input-group>\n                    <b-form-input\n                      id=\"userNickName\"\n                      v-model=\"userNickName\"\n                      trim\n                      placeholder=\"별명을 입력해 주세요.\"\n                      type=\"text\"\n                    ></b-form-input>\n                    <b-input-group-text v-on:click=\"overlapCheck2\" type=\"button\"\n                      >중복확인</b-input-group-text\n                    >\n                  </b-input-group>\n                </b-form-group>\n              </b-col>\n            </b-row>\n\n            <!-- 역락처 -->\n            <b-form-group label=\"연락처 : \" label-for=\"userTel\" class=\"mt-3\">\n              <b-input-group>\n                <b-form-input\n                  id=\"userTel\"\n                  v-model=\"userTel\"\n                  trim\n                  placeholder=\"연락처를 입력해 주세요. ( - 는 빼고 입력해주세요.)\"\n                  type=\"text\"\n                ></b-form-input>\n              </b-input-group>\n            </b-form-group>\n\n            <!-- 주소 -->\n            <div class=\"mt-3 py-4\">\n              <p>주소 :</p>\n              <b-row>\n                <!-- 주소1 -->\n                <b-col>\n                  <b-form-select\n                    v-on:change=\"addre2\"\n                    v-model=\"address1\"\n                    :options=\"addr1\"\n                    class=\"btn btn-secondary\"\n                  ></b-form-select>\n                </b-col>\n\n                <!-- 주소2 -->\n                <b-col>\n                  <b-form-select\n                    v-on:change=\"addre3\"\n                    v-model=\"address2\"\n                    :options=\"addr2\"\n                    class=\"btn btn-secondary\"\n                  ></b-form-select>\n                </b-col>\n\n                <!-- 주소3 -->\n                <b-col>\n                  <b-form-select\n                    v-model=\"address3\"\n                    :options=\"addr3\"\n                    class=\"btn btn-secondary\"\n                  ></b-form-select>\n                </b-col>\n              </b-row>\n            </div>\n\n            <!-- 버튼 -->\n            <b-button-group vertical class=\"d-flex mt-2\">\n              <b-button\n                size=\"lg\"\n                pill\n                block\n                variant=\"primary\"\n                class=\"mt-2\"\n                v-on:click=\"signUp\"\n                >가입하기</b-button\n              >\n            </b-button-group>\n          </b-col>\n        </b-row>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      userId: \"\",\n      userPw: \"\",\n      userPw2: \"\",\n      userName: \"\",\n      userNickName: \"\",\n      userTel: \"\",\n      address1: null,\n      address2: null,\n      address3: null,\n      addr1: [],\n      addr2: [{ value: null, text: \"시/군/구\" }],\n      addr3: [{ value: null, text: \"동\" }],\n      check: false,\n      check2: false,\n    };\n  },\n  beforeCreate() {\n    if (this.$session.get(\"coolUserNo\") != null) {\n      if (this.$session.get(\"coolRank\") == 0) {\n        this.$router.push(\"ManagerPage\");\n      } else {\n        this.$router.push(\"/MyPage\");\n      }\n    }\n  },\n  mounted() {\n    this.$axios\n      .get(\"http://localhost:9000/addr1\")\n      .then((res) => {\n        this.addr1 = res.data;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  },\n  methods: {\n    overlapCheck() {\n      this.$axios\n        .get(\"http://localhost:9000/overlabCheck\", {\n          params: {\n            email: this.userId,\n          },\n        })\n        .then((res) => {\n          if (res.data == true) {\n            alert(\"사용할 수 있는 아이디 입니다.\");\n            this.check = true;\n          } else {\n            alert(\"사용할 수 없는 아이디 입니다.\");\n            this.check = false;\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    overlapCheck2() {\n      this.$axios\n        .get(\"http://localhost:9000/overlabCheck2\", {\n          params: { nickName: this.userNickName },\n        })\n        .then((res) => {\n          if (res.data == true) {\n            alert(\"사용할 수 있는 별명 입니다.\");\n            this.check2 = true;\n          } else {\n            alert(\"사용할 수 없는 별명 입니다.\");\n            this.check2 = false;\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    signUp() {\n      if (this.check == false) {\n        alert(\"이메일 중복확인 해주세요\");\n      } else if (this.check2 == false) {\n        alert(\"별명 중복확인 해주세요\");\n      } else {\n        if (this.userPw == \"\") {\n          alert(\"비밀번호를 입력해 주세요\");\n        } else if (this.userPw2 == \"\") {\n          alert(\"비밀번호 확인을 입력해 주세요\");\n        } else if (this.userName == \"\") {\n          alert(\"이름을 입력해 주세요\");\n        } else if (this.userNickName == \"\") {\n          alert(\"별명을 입력해 주세요\");\n        } else if (this.userTel == \"\") {\n          alert(\"연락처를 입력해 주세요\");\n        } else if (this.address1 == null) {\n          alert(\"주소를 선택해 주세요\");\n        } else {\n          if (this.userPw != this.userPw2) {\n            alert(\"비밀번호를 확인해 주세요\");\n          } else {\n            if (this.address2 == \"없음\") {\n              this.address2 == null;\n            }\n            if (this.address3 == \"없음\") {\n              this.address3 == null;\n            }\n            this.$axios\n              .post(\"http://localhost:9000/signUp\", {\n                email: this.userId,\n                password: this.userPw,\n                name: this.userName,\n                nickName: this.userNickName,\n                tel: this.userTel,\n                addr1: this.address1,\n                addr2: this.address2,\n                addr3: this.address3,\n              })\n              .then(() => {\n                alert(\"회원가입 완료\");\n                this.$router.push({ name: \"Login\", query: { toSignUp: \"1\" } });\n              })\n              .catch((err) => {\n                console.log(err);\n              });\n          }\n        }\n      }\n    },\n    addre2() {\n      this.address2 = null;\n      this.address3 = null;\n      this.$axios\n        .get(\"http://localhost:9000/addr2\", {\n          params: {\n            addr1: this.address1,\n          },\n        })\n        .then((res) => {\n          if (res.data.length == 1) {\n            this.addr2 = [\n              { value: null, text: \"시/군/구\" },\n              { value: \"없음\", text: \"없음\" },\n            ];\n          } else {\n            this.addr2 = res.data;\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    addre3() {\n      this.address3 = null;\n      this.$axios\n        .get(\"http://localhost:9000/addr3\", {\n          params: {\n            addr1: this.address1,\n            addr2: this.address2,\n          },\n        })\n        .then((res) => {\n          if (res.data.length == 1) {\n            this.addr3 = [\n              { value: null, text: \"동\" },\n              { value: \"없음\", text: \"없음\" },\n            ];\n          } else {\n            this.addr3 = res.data;\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n  },\n};\n</script>\n<style>\n</style>"]}]}